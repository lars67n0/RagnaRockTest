@page

@using razor_pages_eksamen_fis.Models
@model razor_pages_eksamen_fis.Pages.RockSound.IndexModel


@{
    ViewData["Title"] = "Home page";
}

<div class="text-center ">
    <h1 class="display-4">Welcome To Ragna Rock Play</h1>

    <p>
        Learn More About Ragna Rock Through Their WebSite =
        <br />
        <a href="https://museumragnarock.dk/ " target="_blank">Ragna Rock Museum // Roskilde</a>.
    </p>




